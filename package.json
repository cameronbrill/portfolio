{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@types/react": "^18.0",
    "@types/react-dom": "^18.0",
    "antd": "^4",
    "classnames": "^2.3.1",
    "highlight.run": "^8.4.1",
    "kbar": "^0.1.0-beta.21",
    "logrocket": "^3.0.0",
    "next": "canary",
    "notyf": "^3.10.0",
    "react": "latest",
    "react-dom": "latest"
  },
  "scripts": {
    "_conc": "concurrently -p '[{time}-{name}-{pid}]' -t 'HH:mm:ss.SS' -c 'auto'",
    "dev": "pnpm _conc 'pnpm:start' 'pnpm:styles:watch'",
    "start": "next dev --turbo",
    "styles": "pnpm typed-scss-modules src --banner '// This file is automatically generated. Do not edit.'",
    "styles:watch": "pnpm styles --watch",
    "build": "next build",
    "lint": "pnpm _conc 'pnpm:lint:biome' 'pnpm:lint:next' 'pnpm:lint:types' 'pnpm:lint:deps'",
    "lint:fix": "pnpm lint:fix:biome && pnpm lint:fix:next && pnpm lint:fix:deps",
    "lint:biome": "biome lint --max-diagnostics 100 ./src",
    "lint:fix:biome": "pnpm lint:biome --apply",
    "lint:fix:unsafe:biome": "pnpm lint:biome --apply-unsafe",
    "lint:next": "next lint",
    "lint:fix:next": "pnpm lint:next --fix",
    "lint:types": "tsc --project tsconfig.json",
    "lint:deps": "pnpm audit",
    "lint:fix:deps": "pnpm lint:deps --fix",
    "lint:ci": "pnpm _conc 'pnpm:lint:ci:biome' 'pnpm:lint:next' 'pnpm:lint:types' 'pnpm:lint:deps'",
    "lint:ci:biome": "biome ci --formatter-enabled=false --linter-enabled=true --organize-imports-enabled=false --max-diagnostics 1000 ./src",
    "format": "pnpm _conc 'pnpm:format:biome' 'pnpm:format:imports'",
    "format:fix": "pnpm format:fix:biome && pnpm format:fix:imports",
    "format:biome": "biome format ./src",
    "format:fix:biome": "pnpm format:biome --write",
    "format:imports": "prettier src --check",
    "format:fix:imports": "pnpm format:imports --write",
    "format:ci": "pnpm _conc 'pnpm:format:ci:biome' 'pnpm:format:imports'",
    "format:ci:biome": "biome ci --formatter-enabled=true --linter-enabled=false --organize-imports-enabled=false --max-diagnostics 1000 ./src"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "20.11.19",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "prettier": "3.2.5",
    "sass": "^1.71.0",
    "turbo": "^1.12.4",
    "typed-scss-modules": "^8.0.0",
    "typescript": "^4.6.4"
  },
  "packageManager": "pnpm@8.15.3+sha256.fc4a49bd609550a41e14d20efbce802a4b892aa4cac877322de2f0924f122991"
}
