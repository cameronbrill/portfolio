{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@types/react": "^18.0",
    "@types/react-dom": "^18.0",
    "antd": "^4",
    "classnames": "^2.3.1",
    "highlight.run": "^4.2.5",
    "kbar": "^0.1.0-beta.21",
    "logrocket": "^3.0.0",
    "next": "canary",
    "notyf": "^3.10.0",
    "react": "latest",
    "react-dom": "latest"
  },
  "scripts": {
    "s": "concurrently 'yarn start' 'yarn styles:watch'",
    "start": "next",
    "styles": "yarn typed-scss-modules src --banner '// This file is automatically generated. Do not edit.'",
    "styles:watch": "yarn styles --watch",
    "build": "next build",
    "lint": "concurrently -n biome,next,types 'yarn lint:biome' 'yarn lint:next' 'yarn lint:types'",
    "lint:fix": "yarn lint:fix:biome && yarn lint:fix:next",
    "lint:biome": "biome lint --max-diagnostics 100 ./src",
    "lint:fix:biome": "yarn lint --apply",
    "lint:fix:unsafe:biome": "yarn lint --apply-unsafe",
    "lint:next": "next lint",
    "lint:fix:next": "yarn lint:next --fix",
    "lint:types": "tsc --project tsconfig.json",
    "lint:ci": "concurrently -n biome,next,types 'yarn lint:ci:biome' 'yarn lint:next' 'yarn lint:types'",
    "lint:ci:biome": "biome ci --formatter-enabled=false --linter-enabled=true --organize-imports-enabled=false --max-diagnostics 1000 ./src",
    "format": "concurrently -n biome,imports 'yarn format:biome' 'yarn format:imports'",
    "format:fix": "yarn format:fix:biome && yarn format:fix:imports",
    "format:biome": "biome format ./src",
    "format:fix:biome": "yarn format --write",
    "format:imports": "prettier src --check",
    "format:fix:imports": "yarn imports --write",
    "format:ci": "concurrently -n biome,imports 'yarn format:ci:biome' 'yarn format:imports'",
    "format:ci:biome": "biome ci --formatter-enabled=true --linter-enabled=false --organize-imports-enabled=false --max-diagnostics 1000 ./src"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "concurrently": "^6.3.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "prettier": "3.2.5",
    "sass": "^1.51.0",
    "typed-scss-modules": "^6",
    "typescript": "^4.6.4"
  },
  "packageManager": "yarn@3.1.1"
}
