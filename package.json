{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@types/react": "^18.0",
    "@types/react-dom": "^18.0",
    "antd": "^4",
    "classnames": "^2.3.1",
    "highlight.run": "^4.2.5",
    "kbar": "^0.1.0-beta.21",
    "logrocket": "^3.0.0",
    "next": "canary",
    "notyf": "^3.10.0",
    "react": "latest",
    "react-dom": "latest"
  },
  "scripts": {
    "s": "concurrently 'yarn start' 'yarn styles:watch'",
    "start": "next",
    "styles": "yarn typed-scss-modules src --banner '// This file is automatically generated. Do not edit.'",
    "styles:watch": "yarn styles --watch",
    "build": "next build",
    "lint-slow": "next lint",
    "lint": "yarn run -T biome lint ./src",
    "lint:fix": "yarn run -T biome lint --apply ./src",
    "lint:fix:unsafe": "yarn run -T biome lint --apply-unsafe ./src",
    "type-check": "yarn run -T tsc --project tsconfig.json"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "concurrently": "^6.3.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "sass": "^1.51.0",
    "typed-scss-modules": "^6",
    "typescript": "^4.6.4"
  },
  "packageManager": "yarn@3.1.1"
}
