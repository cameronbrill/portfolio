{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@types/react": "^18.0",
    "@types/react-dom": "^18.0",
    "antd": "^4",
    "classnames": "^2.3.1",
    "highlight.run": "^8.4.1",
    "kbar": "^0.1.0-beta.21",
    "logrocket": "^3.0.0",
    "next": "canary",
    "notyf": "^3.10.0",
    "react": "latest",
    "react-dom": "latest"
  },
  "scripts": {
    "start": "yarn _conc 'yarn:build:ts:watch' 'yarn:build:scss:watch'",
    "build": "yarn _conc 'yarn:build:ts' 'yarn:build:scss'",
    "lint": "yarn _conc 'yarn:lint:biome' 'yarn:lint:next' 'yarn:lint:types' 'yarn:lint:deps'",
    "lint:fix": "yarn lint:fix:biome && yarn lint:fix:next && yarn lint:fix:deps",
    "format": "yarn _conc 'yarn:format:biome' 'yarn:format:imports'",
    "format:fix": "yarn format:fix:biome && yarn format:fix:imports",
    "build:ts": "next build",
    "build:ts:watch": "next dev --turbo",
    "build:scss": "yarn typed-scss-modules src --banner '// This file is automatically generated. Do not edit.'",
    "build:scss:watch": "yarn build:scss --watch",
    "lint:biome": "biome lint --max-diagnostics 100 ./src",
    "lint:fix:biome": "yarn lint:biome --apply",
    "lint:fix:unsafe:biome": "yarn lint:biome --apply-unsafe",
    "lint:next": "next lint",
    "lint:fix:next": "yarn lint:next --fix",
    "lint:deps": "yarn audit",
    "lint:fix:deps": "yarn lint:deps --fix",
    "lint:types": "tsc --project tsconfig.json",
    "lint:ci": "yarn _conc 'yarn:lint:ci:biome' 'yarn:lint:next' 'yarn:lint:types' 'yarn:lint:deps'",
    "lint:ci:biome": "biome ci --formatter-enabled=false --linter-enabled=true --organize-imports-enabled=false --max-diagnostics 1000 ./src",
    "format:biome": "biome format ./src",
    "format:fix:biome": "yarn format:biome --write",
    "format:imports": "prettier src --check",
    "format:fix:imports": "yarn format:imports --write",
    "format:ci": "yarn _conc 'yarn:format:ci:biome' 'yarn:format:imports'",
    "format:ci:biome": "biome ci --formatter-enabled=true --linter-enabled=false --organize-imports-enabled=false --max-diagnostics 1000 ./src",
    "_conc": "concurrently -p '[{time}-{name}-{pid}]' -t 'HH:mm:ss.SS' -c 'auto'"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "20.11.19",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "prettier": "3.2.5",
    "sass": "^1.71.0",
    "turbo": "^1.12.4",
    "typed-scss-modules": "^8.0.0",
    "typescript": "^4.6.4"
  },
  "packageManager": "yarn@4.1.0"
}
